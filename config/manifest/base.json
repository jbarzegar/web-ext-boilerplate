{
  "manifest_version": 2,
  "name": "Test-ext",
  "short_name": "test",
  "version": "1.0.0",
  "minimum_chrome_version": "45",
  "description": "A test ext built with webpack 4 and react",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Test-ext"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "proxy",
    "management",
    "tabs",
    "clipboardWrite",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "activeTab",
    "background",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "privacy",
    "webNavigation",
    "notifications",
    "alarms",
    "cookies"
  ],

  "background": {
    "page": "background.html"
  }
}
